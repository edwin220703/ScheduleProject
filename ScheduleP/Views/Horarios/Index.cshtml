@model IEnumerable<ScheduleP.Models.Horario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DiaSemana)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraFin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAsignaturaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAulaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdDocenteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdGrupoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DiaSemana)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraFin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAsignaturaNavigation.IdAsignatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAulaNavigation.IdAula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDocenteNavigation.IdDocente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdGrupoNavigation.IdGrupo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdHorario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdHorario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdHorario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
